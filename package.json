{
  "name": "react-well-plates",
  "version": "3.1.3",
  "description": "Render well plates in react",
  "main": "lib/src/index.js",
  "module": "lib-es/src/index.js",
  "types": "lib-es/src/index.d.ts",
  "files": [
    "lib/src",
    "lib-es/src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc && tsc -p tsconfig.es.json",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-es",
    "npm:publish": "npm t && npm publish --access public && git push --follow-tags",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run deploy",
    "watch": "tsc -p tsconfig.es.json -w",
    "dev": "start-storybook --no-dll -p 6006",
    "build-storybook": "build-storybook --no-dll",
    "eslint": "eslint --ext ts,tsx src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "npm run eslint && npm run check-types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/react-well-plates.git"
  },
  "keywords": [],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/react-well-plates/issues"
  },
  "homepage": "https://github.com/zakodium/react-well-plates#readme",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "prettier": {
    "trailingComma": "all",
    "arrowParens": "always",
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/react": "^6.0.26",
    "@types/react": "^16.9.53",
    "@zakodium/eslint-config": "^2.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.11.0",
    "prettier": "^2.1.2",
    "react": "^16.",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "well-plates": "^4.1.0"
  }
}
